<Page loaded="loaded">
	<StackLayout orientation="vertical">
		<TextField id="login-username" hint="Username" />
		<TextField id="login-password" hint="Password" secure="true" />

		<Button text="LOG IN" tap="login" />
		<Button text="FIRST TIME HERE? REGISTER" cssClass="register-link" tap="register" />
	</StackLayout>
</Page>